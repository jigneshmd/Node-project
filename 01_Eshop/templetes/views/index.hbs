<!-- Product Section Begin -->
<section class="product spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                    <h4>New product</h4>
                </div>
            </div>          
        </div>
        <div class="row property__gallery">
            {{#each prodata}}
            <div class="col-lg-3 col-md-0 col-sm-6 mix women">
                <div class="product__item">
                    <div class="product__item__pic set-bg" src="productimg/{{this.img}}">
                            <img height="250" width="250" src="productimg/{{this.img}}" alt="{{this.img}}">
                        <ul class="product__hover">
                            <li><a href="productimg/{{this.img}}" class="image-popup"><span class="arrow_expand"></span></a></li>
                            <li><a onclick="addCart('{{this._id}}')"><span class="icon_bag_alt"></span></a></li>   
                            <li><a href="details?pid={{this._id}}"><span class="fa fa-eye"></span></a></li>                       
                        </ul>
                    </div>                 
                    <div class="product__item__text">
                        <h6><a href="#">{{this.pname}}</a></h6>
                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__price">Rs. {{this.price}}</div>
                   
                    </div>
                </div>
            </div>
           {{/each}}
        </div>
    </div>
</section>
